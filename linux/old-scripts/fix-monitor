xrandr --output HDMI-1-1 --auto --right-of HDMI-1-0 # second monitor support
xrandr --output HDMI-1-2 --auto --right-of HDMI-1-1 # second monitor support
xrandr --output HDMI-1-3 --auto --right-of HDMI-1-2 # second monitor support
xrandr --output HDMI-1-4 --auto --right-of HDMI-1-3 # second monitor support
xrandr --output HDMI-1-0 --auto --right-of eDP-1 # second monitor support
